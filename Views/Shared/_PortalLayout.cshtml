<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EC-ASP.NET</title>
    <script type="importmap"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.9.0/highlight.min.js"></script>
	<link href="https://cdn.jsdelivr.net/npm/quill@2.0.3/dist/quill.snow.css" rel="stylesheet" />
	<script src="https://cdn.jsdelivr.net/npm/quill@2.0.3/dist/quill.js"></script>

    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
	
</head>
<body>
	<div class="portal">
		<div class="logotype">
			@await Html.PartialAsync("Partials/Components/_AlphaLogo")
		</div>
		<div class="side">
			@await Html.PartialAsync("Partials/Components/_Navlinks")
		</div>
		<div class="top">
						@await Html.PartialAsync("Partials/Components/_ProfileAndSettings")
		</div>

		<div class="main">
		
			
			@RenderBody()
		</div>
	</div>






    @await RenderSectionAsync("Scripts", required: false)
    <script src="~/js/site.js" asp-append-version="true"></script>
</body>
</html>
