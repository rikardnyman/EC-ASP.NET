<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EC-ASP.NET</title>
    <script type="importmap"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/InlämningASP.NET.styles.css" asp-append-version="true" />
	<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
	
</head>
<body>
	<div class="portal">
		<div class="logotype">
			@await Html.PartialAsync("Partials/Components/_AlphaLogo")
		</div>
		<div class="side">
			@await Html.PartialAsync("Partials/Components/_Navlinks")
		</div>
		<div class="top">
						@await Html.PartialAsync("Partials/Components/_ProfileAndSettings")
		</div>

		<div class="main">
		
			
			@RenderBody()
		</div>
	</div>





    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
